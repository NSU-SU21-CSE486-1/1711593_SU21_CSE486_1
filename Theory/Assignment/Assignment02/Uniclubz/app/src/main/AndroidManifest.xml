<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.fahemaSultana.project02">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication">
        <activity android:name="com.fahemaSultana.project02.profile.UserProfileActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name="com.fahemaSultana.project02.signUp.MainActivity">

        </activity>
        <activity
            android:name="com.fahemaSultana.project02.signUp.Student_Profile"
            android:parentActivityName="com.fahemaSultana.project02.signUp.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.fahemaSultana.project02.signUp.MainActivity" />
        </activity>
        <activity
            android:name="com.fahemaSultana.project02.signUp.profilesettings"
            android:parentActivityName="com.fahemaSultana.project02.signUp.Student_Profile">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.fahemaSultana.project02.signUp.MainActivity" />
        </activity>
    </application>

</manifest>